echo "Lorem ipsum" >file1
cat <file1
# Lorem ipsum
echo "Lorem ipsum" > file1 | cat -e > file2
cat <file1 <file2
cat <file2 <file1
# Lorem ipsum
export TEST="> file1"
echo "lorem" $TEST | cat -e > file2
cat <file2
# lorem > file1$
echo "dolor sit amet" >file2
<file1 <file2 cat -e | cat -e
# dolor sit amet$$
>file3 cat >file4 <file1 <file2
cat <file3
cat <file4
# dolor sit amet
echo Lorem | cat | nl >file3 | <file1 nl >>file3 | <file2 nl >>file3
cat file3
    #  1  Lorem
    #  1  Lorem ipsum
    #  1  dolor sit amet
#zsh:
    #  1  Lorem
    #  1       1  Lorem
    #  2  Lorem ipsum
    #  1       1       1  Lorem
    #  2       2  Lorem ipsum
    #  3  dolor sit amet
>file1 echo > file2 >>file1>file1>file3>>file1 Lorem >file3 >file2 ipsum >     file1
cat file1
# Lorem ipsum